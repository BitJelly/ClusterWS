{
  "name": "clusterws",
  "version": "0.1.0",
  "description": "ClusterWS - minimal node js http and real-time framework which allows easily scale WebSocket(uWS - one of the fastest WebSocket libraries) between node clusters and utilize all available CPU",
  "main": "index.js",
  "author": "Dmitrii Goriunov <goriunovd@gmail.com>",
  "scripts": {
    "test": "mocha -r ts-node/register fp-library/tests/*.ts",
    "start": "npm run build:dev && npm run boot",
    "boot": "cross-env DEBUG=true node test/server.js",
    "build:prod": "webpack",
    "build:dev": "cross-env WEBPACK_ENV=debug webpack",
    "build:publish": "npm run build:prod && cd dist && npm publish && cd ..",
    "build:changelog": "cross-env NODE_ENV=...0.1.0 node information/changelog.js"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "websocket",
    "socket",
    "cluster",
    "nodejs",
    "websocketcluster",
    "clusterws",
    "js",
    "scale",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/goriunov/ClusterWS"
  },
  "devDependencies": {
    "@types/node": "^8.0.5",
    "@types/uws": "^0.13.0",
    "copy-pkg-json-webpack-plugin": "0.0.29",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "ts-loader": "^2.2.1",
    "typescript": "^2.4.1",
    "webpack": "^3.1.0"
  },
  "dependencies": {
    "uws": "^8.14.1"
  }
}
