    "cross-env": "^5.0.1",
    "express": "^4.15.4",
    "ts-loader": "^2.2.1",
    "typescript": "^2.4.1",
    "webpack": "^3.1.0"
  },
  "dependencies": {
    "uws": "^8.14.1"
  }
